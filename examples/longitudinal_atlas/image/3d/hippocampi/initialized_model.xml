<?xml version="1.0" ?>
<model deformetrica-min-version="3.0.0">
	    
	<model-type>LongitudinalAtlas</model-type>
	    
	<dimension>3</dimension>
	    
	<template>
		        
		<object id="right_hippocampus">
			            
			<deformable-object-type>Image</deformable-object-type>
			            
			<filename>data/ForInitialization__Template_right_hippocampus__FromAtlasAndShooting.nii</filename>
			            
			<noise-std>1.0000</noise-std>
			        
		</object>
		    
	</template>
	    
	<deformation-parameters>
		        
		<kernel-type>cudaexact</kernel-type>
		        
		<kernel-width>10</kernel-width>
		        
		<concentration-of-timepoints>5</concentration-of-timepoints>
		        
		<number-of-timepoints>6</number-of-timepoints>
		        
		<number-of-sources>2</number-of-sources>
		    
	</deformation-parameters>
	<initial-control-points>data/ForInitialization__ControlPoints__FromAtlasAndShooting.txt</initial-control-points>
	<initial-momenta>data/ForInitialization__Momenta__FromRegressionsAndShooting.txt</initial-momenta>
	<initial-onset-ages>data/ForInitialization__OnsetAges__FromHeuristic.txt</initial-onset-ages>
	<initial-log-accelerations>data/ForInitialization__LogAccelerations__FromHeuristic.txt</initial-log-accelerations>
	<initial-modulation-matrix>data/ForInitialization__ModulationMatrix__FromICA.txt</initial-modulation-matrix>
	<initial-sources>data/ForInitialization__Sources__FromICA.txt</initial-sources>
</model>

