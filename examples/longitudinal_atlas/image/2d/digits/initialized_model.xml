<?xml version="1.0" ?>
<model deformetrica-min-version="3.0.0">
	    
	<model-type>LongitudinalAtlas</model-type>
	    
	<dimension>2</dimension>
	    
	<template>
		        
		<object id="img">
			            
			<deformable-object-type>Image</deformable-object-type>
			            
			<filename>data/ForInitialization__Template_img__FromAtlasAndShooting.png</filename>
			            
			<noise-std>0.1</noise-std>
			        
		</object>
		    
	</template>
	    
	<deformation-parameters>
		        
		<concentration-of-timepoints>5</concentration-of-timepoints>
		        
		<kernel-type>exact</kernel-type>
		        
		<kernel-width>4</kernel-width>
		        
		<number-of-sources>1</number-of-sources>
		    
	</deformation-parameters>
	<initial-control-points>data/ForInitialization__ControlPoints__FromAtlasAndShooting.txt</initial-control-points>
	<initial-momenta>data/ForInitialization__Momenta__FromRegressionsAndShooting.txt</initial-momenta>
</model>

