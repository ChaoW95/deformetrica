<?xml version="1.0" ?>
<model>
	    
	<model-type>LongitudinalAtlas</model-type>
	    
	<dimension>2</dimension>
	    
	<template>
		        
		<object id="starman">
			            
			<deformable-object-type>PolyLine</deformable-object-type>
			            
			<attachment-type>Landmark</attachment-type>
			                        
			<filename>data/ForInitialization__Template_starman__FromAtlasAndShooting.vtk</filename>
			            
			<noise-std>1.0000</noise-std>
			                                
		</object>
		    
	</template>
	    
	<deformation-parameters>
		        
		<kernel-type>torch</kernel-type>
		        
		<kernel-width>1</kernel-width>
		                
		<number-of-sources>4</number-of-sources>
		    
	</deformation-parameters>
	                        
	<initial-control-points>data/ForInitialization__ControlPoints__FromAtlasAndShooting.txt</initial-control-points>
	<initial-momenta>data/ForInitialization__Momenta__RescaledWithLongitudinalRegistration.txt</initial-momenta>
	<initial-acceleration-std>0.0245</initial-acceleration-std>
	<initial-onset-ages>data/ForInitialization__OnsetAges__FromLongitudinalRegistration.txt</initial-onset-ages>
	<initial-accelerations>data/ForInitialization__Accelerations__FromLongitudinalRegistration.txt</initial-accelerations>
	<initial-modulation-matrix>data/ForInitialization__ModulationMatrix__FromICA.txt</initial-modulation-matrix>
	<initial-sources>data/ForInitialization__Sources__FromLongitudinalRegistration.txt</initial-sources>
	<initial-time-shift-std>0.5143</initial-time-shift-std>
</model>

