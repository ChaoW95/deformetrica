<?xml version="1.0"?>
<model deformetrica-min-version="3.0.0">

    <model-type>LongitudinalRegistration</model-type>
    <dimension>2</dimension>

    <template>
        <object id="starman">
            <deformable-object-type>PolyLine</deformable-object-type>
            <attachment-type>Varifold</attachment-type>
            <kernel-type>cudaexact</kernel-type>
            <kernel-width>1</kernel-width>
            <filename>data/SimulatedData__Parameters__Template.vtk</filename>
            <noise-std>0.1</noise-std>
        </object>
    </template>

    <deformation-parameters>
        <kernel-type>cudaexact</kernel-type>
        <kernel-width>1</kernel-width>
        <t0>70</t0>
    </deformation-parameters>

    <initial-control-points>data/SimulatedData__Parameters__ControlPoints.txt</initial-control-points>
    <initial-momenta>data/SimulatedData__Parameters__Momenta.txt</initial-momenta>
    <initial-modulation-matrix>data/SimulatedData__Parameters__ProjectedModulationMatrix.txt</initial-modulation-matrix>
    <initial-time-shift-std>1</initial-time-shift-std>
    <initial-log-acceleration-std>0.1</initial-log-acceleration-std>

    <!-- <nb-sources>2</nb-sources> -->

</model>
