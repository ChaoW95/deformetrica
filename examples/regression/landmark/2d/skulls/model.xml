<?xml version="1.0"?>
<model deformetrica-min-version="3.0.0">

    <model-type>Regression</model-type>
    <dimension>2</dimension>

    <template>
        <object id="skull">
            <deformable-object-type>Polyline</deformable-object-type>
            <attachment-type>Varifold</attachment-type>
            <noise-std>5.</noise-std>
            <kernel-width>20.0</kernel-width>
            <kernel-type>exact</kernel-type>
            <filename>data/template.vtk</filename>
        </object>
    </template>

    <deformation-parameters>
        <kernel-width>30</kernel-width>
        <kernel-type>exact</kernel-type>
        <number-of-timepoints>5</number-of-timepoints>
    </deformation-parameters>

</model>
