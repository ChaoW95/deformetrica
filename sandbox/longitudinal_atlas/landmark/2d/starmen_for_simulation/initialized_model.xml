<?xml version="1.0" ?>
<model>
	    
	<model-type>LongitudinalRegistration</model-type>
	    
	<dimension>2</dimension>
	    
	<template>
		        
		<object id="starman">
			            
			<deformable-object-type>PolyLine</deformable-object-type>
			            
			<attachment-type>Varifold</attachment-type>
			            
			<kernel-type>keops</kernel-type>
			            
			<kernel-width>1</kernel-width>
			            
			<filename>data/ForInitialization__Template_starman__FromAtlasAndShooting.vtk</filename>
			        
			<noise-std>0.3015</noise-std>
		</object>
		    
	</template>
	    
	<deformation-parameters>
		        
		<kernel-type>keops</kernel-type>
		        
		<kernel-width>1</kernel-width>
		        
		<number-of-sources>4</number-of-sources>
		    
	</deformation-parameters>
	<initial-control-points>data/ForInitialization__ControlPoints__FromAtlasAndShooting.txt</initial-control-points>
	<initial-momenta>data/ForInitialization__Momenta__FromRegressionsAndShooting.txt</initial-momenta>
	<initial-modulation-matrix>data/ForInitialization__ModulationMatrix__FromAtlasAndPCA.txt</initial-modulation-matrix>
</model>

